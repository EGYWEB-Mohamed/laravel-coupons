{
    "name": "aberkanidev/laravel-coupons",
    "description": "Coupon generator for Laravel",
    "keywords": [
        "aberkanidev",
        "laravel-coupons",
        "vouchers",
        "laravel",
        "php"
    ],
    "homepage": "https://github.com/aberkanimed/laravel-coupons",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mohamed Aberkani",
            "email": "aberkanidev@gmail.com",
            "homepage": "https://aberkanidev.com",
            "role": "Web Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.0|^11.0",
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "aberkanidev\\Coupons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "aberkanidev\\Coupons\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "aberkanidev\\Coupons\\CouponsServiceProvider"
            ],
            "aliases": {
                "Coupons": "aberkanidev\\Coupons\\Facades\\Coupons"
            }
        }
    }
}
